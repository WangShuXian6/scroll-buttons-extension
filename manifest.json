{
    "manifest_version": 3,
    "name": "滚动按钮插件",
    "version": "1.0",
    "description": "在浏览器右侧悬浮两个按钮，点击后可按配置滚动页面。",
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "permissions": [
      "storage"
    ],
    "options_page": "options.html",
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "css": ["styles.css"]
      }
    ]
  }
  